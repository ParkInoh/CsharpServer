<?xml version="1.0" encoding="utf-8" ?>
<!-- C_: 클라이언트 -> 서버 / S_: 서버 -> 클라이언트 -->
<PDL>
	<packet name="C_PlayerInfoReq">
		<byte name="testByte"/>
		<long name="playerId"/>
		<string name="name"/>
		<list name="skill">
			<int name="id"/>
			<short name="level"/>
			<float name="duration"/>
			<list name="attribute">
				<int name="att"/>
			</list>
		</list>
	</packet>
	<packet name="S_Test">
		<int name="testInt"/>
	</packet>
</PDL>